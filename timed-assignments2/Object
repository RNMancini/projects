hotel = { 
occupied_rooms: 10
amenities: heat
totalRooms: 80

availableRooms() {
if (occupied_rooms < 1)
then return (totalRooms);
if (occupied_rooms > 1) then
return (occupied_rooms - totalRooms);
}

bookRoom() {
if (occupied_rooms > 1) 
then return 
(totalRooms - occupied_rooms) 
}

relinquishARoom() { 
if (occupied_rooms - 1) 
return 
(totalRooms - (occupied_rooms - 1)
}
}